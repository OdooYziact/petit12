<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
	<record id="view_procurement_compute_wizard_itgwana" model="ir.ui.view">
        <field name="name">Run Schedulers Manually</field>
        <field name="model">stock.scheduler.compute</field>
        <field name="inherit_id" ref="stock.view_procurement_compute_wizard"/>
        <field name="arch" type="xml">
            <xpath expr="//footer" position="before">
            	<label for="partner_id"/>
            	<field name="partner_id" context="{'search_default_supplier':1, 'default_supplier':1, 'default_customer':0}" domain="[('supplier','=',True)]"/>
            </xpath>
        </field>
    </record>
</odoo>